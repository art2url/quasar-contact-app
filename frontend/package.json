{
  "name": "quasar-contact-app-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "format": "prettier --write src/**/*.{ts,html,css,scss}",
    "format:check": "prettier --check src/**/*.{ts,html,css,scss}",
    "style:css": "stylelint src/**/*.{css,scss}",
    "style:css:fix": "stylelint src/**/*.{css,scss} --fix",
    "typecheck": "ng build --configuration development --aot=true --source-map=false --output-path=tmp --delete-output-path=true",
    "style": "npm run lint && npm run format:check && npm run style:css",
    "style:fix": "npm run lint:fix && npm run format && npm run style:css:fix",
    "test:ci": "ng test --watch=false --browsers=ChromeHeadless"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.5",
    "@angular/cdk": "^19.2.16",
    "@angular/common": "^19.2.5",
    "@angular/compiler": "^19.2.5",
    "@angular/core": "^19.2.5",
    "@angular/forms": "^19.2.5",
    "@angular/material": "^19.2.16",
    "@angular/platform-browser": "^19.2.5",
    "@angular/platform-browser-dynamic": "^19.2.5",
    "@angular/router": "^19.2.5",
    "hammerjs": "^2.0.8",
    "rxjs": "~7.8.0",
    "socket.io-client": "^4.8.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.6",
    "@angular/cli": "^19.2.6",
    "@angular/compiler-cli": "^19.2.5",
    "@ngx-env/builder": "^19.0.4",
    "@types/hammerjs": "^2.0.46",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^22.15.18",
    "angular-eslint": "19.4.0",
    "eslint": "^9.26.0",
    "jasmine-core": "~5.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.5.2",
    "typescript-eslint": "8.32.0"
  },
  "optionalDependencies": {
    "@rollup/rollup-darwin-arm64": "^4.52.5",
    "@rollup/rollup-linux-x64-musl": "4.9.5"
  }
}
